	</servlet>
	<servlet-mapping>
		<servlet-name>FileController</servlet-name>
		<url-pattern>/files/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>ScheduleController</servlet-name>
	    <servlet-class>schedule.controller.ScheduleController</servlet-class>
	  </servlet>
	  <servlet-mapping>
	    <servlet-name>ScheduleController</servlet-name>
	    <url-pattern>/schedule/*</url-pattern>
	  </servlet-mapping>
	  
	  	<servlet>
	    <servlet-name>AdminController</servlet-name>
	    <servlet-class>admin.controller.AdminController</servlet-class>
	  </servlet>
	  <servlet-mapping>
	    <servlet-name>AdminController</servlet-name>
	    <url-pattern>/admin/*</url-pattern>
	  </servlet-mapping>
	
  
</web-app>